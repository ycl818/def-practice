   ],
   "source": [
    "def gcd(m,n):\n",
    "    if n==0:\n",
    "        return m\n",
    "    else:\n",
    "        return gcd(n, m%n)\n",
    "a,b=input('Input two numbers with \",\" in the middle').split(',')\n",
    "a=int(a)\n",
    "b=int(b)\n",
    "print(a,'and',b,\"'s greatest common divisor is\",gcd(a,b))"
   ]
